<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>servlet-training</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FirstServlet</display-name>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>com.acme.servlet.FirstServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/servlets/first</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/first.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CoffeServlet</display-name>
    <servlet-name>CoffeServlet</servlet-name>
    <servlet-class>com.acme.servlet.CoffeServlet</servlet-class>
    <init-param>
      <param-name>coffeeTime</param-name>
      <param-value>2012-09-10 15:45</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>CoffeServlet</servlet-name>
    <url-pattern>/coffee/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CoffeServlet</servlet-name>
    <url-pattern>*.cf</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CoffeServlet</servlet-name>
    <url-pattern>/furcsa*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>gohomeServlet</display-name>
    <servlet-name>gohomeServlet</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <description>message to display</description>
      <param-name>message</param-name>
      <param-value>Time left till end of day</param-value>
    </init-param>
    <init-param>
      <description>The time in yyyy-MM-dd HH:mm format</description>
      <param-name>breakTime</param-name>
      <param-value>2012-09-10 16:50</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <servlet-name>lastCoffee</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <param-name>message</param-name>
      <param-value>Time left till last coffee break</param-value>
    </init-param>
    <init-param>
      <param-name>breakTime</param-name>
      <param-value>2012-09-10 15:45</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <servlet-name>lastCoffeeLate</servlet-name>
    <servlet-class>com.acme.servlet.BreakServlet</servlet-class>
    <init-param>
      <param-name>message</param-name>
      <param-value>You are late by</param-value>
    </init-param>
    <init-param>
      <param-name>breakTime</param-name>
      <param-value>2012-09-10 16:00</param-value>
    </init-param>
    <init-param>
      <param-name>isLate</param-name>
      <param-value>true</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>gohomeServlet</servlet-name>
    <url-pattern>/gohome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lastCoffee</servlet-name>
    <url-pattern>/lastCoffee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lastCoffeeLate</servlet-name>
    <url-pattern>/lastCoffeeLate</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CVServlet</display-name>
    <servlet-name>CVServlet</servlet-name>
    <servlet-class>com.acme.servlet.CVServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CVServlet</servlet-name>
    <url-pattern>/CVServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ImageServlet</display-name>
    <servlet-name>ImageServlet</servlet-name>
    <servlet-class>com.acme.servlet.ImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageServlet</servlet-name>
    <url-pattern>/img</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImageServlet</servlet-name>
    <url-pattern>/test.svg</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GirlsServlet</display-name>
    <servlet-name>GirlsServlet</servlet-name>
    <servlet-class>com.acme.servlet.GirlsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GirlsServlet</servlet-name>
    <url-pattern>/girls</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ShowGirl</display-name>
    <servlet-name>ShowGirl</servlet-name>
    <servlet-class>com.acme.servlet.ShowGirl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowGirl</servlet-name>
    <url-pattern>/showGirl</url-pattern>
  </servlet-mapping>
</web-app>